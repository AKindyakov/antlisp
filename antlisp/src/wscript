#!/usr/bin/env python
# encoding: utf-8


def build(ctx):
    ctx.stlib(
        target="antlisp-src",
        source = [
            "stack_machine.cpp",
        ],
        use=[
        ],
    )
    ctx.program(
        target="stack_machine_ut",
        source="stack_machine_ut.cpp",
        use=[
            "antlisp-src"
        ],
    )
    ctx(
        rule="${SRC}",
        source="stack_machine_ut",
    )
