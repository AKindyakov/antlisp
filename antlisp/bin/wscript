#!/usr/bin/env python
# encoding: utf-8


def build(ctx):
    ctx.program(
        target="antlisp",
        source = [
            "args.cpp",
            "main.cpp",
        ],
        lib=[
            "boost_filesystem",
            "boost_program_options",
            "boost_system",
        ],
        use=[
            "antlisp-lib",
        ],
    )
